@using Renato.ECommerce.WebUI.Models

@model FeaturedProductsViewModel

@{
    ViewData["Title"] = "Home Page";
}


<h2>
    Featured Products
</h2>

<div>

    @foreach (ProductViewModel product in this.Model.Products)
    {
        <div>
            @product.SummaryText
        </div>
    }

</div>